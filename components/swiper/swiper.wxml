<view class="swiper">
  <swiper
    class="myswiper"
    indicator-dots="{{ indicatorDots }}"
    autoplay="{{ autoplay }}"
    interval="{{ interval }}"
    duration="{{ duration }}"
    indicator-color="#fff"
    indicator-active-color="#fa2c19"
  >
    <swiper-item>
      <image mode="aspectFill" src="./images/bike.jpeg"></image>
    </swiper-item>
    <swiper-item>
      <image mode="aspectFill" src="./images/bike2.jpeg"></image>
    </swiper-item>
  </swiper>
</view>
